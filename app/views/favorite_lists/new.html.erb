<h1>Créer une Nouvelle Liste de Favoris</h1>

<%= form_with model: @favorite_list, url: favorite_lists_path, method: :post, local: true do |form| %>
  <div>
    <%= form.label :title %>
    <%= form.text_field :title %>
  </div>
  <div>
    <%= form.label :localisation %>
    <%= form.text_field :localisation %>
  </div>
  <%= form.submit 'Créer la liste de favoris' %>
<% end %>
